1.WAP to find a the answer given formula (x+y)2.

#include <stdio.h>

int main() {
    int x, y, ans;
    printf("Enter x and y: ");
    scanf("%d %d", &x, &y);

    ans = (x + y) * (x + y);
    printf("Result = %d", ans);

    return 0;
}

---------------------------------------------------
2.WAP to find a the answer given formula (x-y)2

#include <stdio.h>

int main() {
    int x, y, ans;
    printf("Enter x and y: ");
    scanf("%d %d", &x, &y);

    ans = (x - y) * (x - y);
    printf("Result = %d", ans);

    return 0;
}

---------------------------------------------------
3.WAP to find a the answer given formula (x+y)3

#include <stdio.h>

int main() {
    int x, y, ans;
    printf("Enter x and y: ");
    scanf("%d %d", &x, &y);

    ans = (x + y) * (x + y) * (x + y);
    printf("Result = %d", ans);

    return 0;
}

---------------------------------------------------
4.WAP to find a the answer given formula (x-y)3

#include <stdio.h>

int main() {
    int x, y, ans;
    printf("Enter x and y: ");
    scanf("%d %d", &x, &y);

    ans = (x - y) * (x - y) * (x - y);
    printf("Result = %d", ans);

    return 0;
}

---------------------------------------------------
5.WAP to find a the answer given formula (x+y+z)3

#include <stdio.h>

int main() {
    int x, y, z, ans;
    printf("Enter x, y and z: ");
    scanf("%d %d %d", &x, &y, &z);

    ans = (x + y + z) * (x + y + z) * (x + y + z);
    printf("Result = %d", ans);

    return 0;
}

---------------------------------------------------
6.WAP to find a the answer given formula (x-y-z)3

#include <stdio.h>

int main() {
    int x, y, z, ans;
    printf("Enter x, y and z: ");
    scanf("%d %d %d", &x, &y, &z);

    ans = (x - y - z) * (x - y - z) * (x - y - z);
    printf("Result = %d", ans);

    return 0;
}


---------------------------------------------------
7.WAP to find a the answer given formula (x+y+z)2
#include <stdio.h>

int main() {
    int x, y, z, ans;
    printf("Enter x, y and z: ");
    scanf("%d %d %d", &x, &y, &z);

    ans = (x + y + z) * (x + y + z);
    printf("Result = %d", ans);

    return 0;
}

